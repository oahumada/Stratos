{
    "$schema": "https://www.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "test:acceptance": "cypress run",
        "build": "vite build",
        "build:ssr": "vite build && vite build --ssr",
        "dev": "vite",
        "test:e2e": "playwright test",
        "test:e2e:headed": "xvfb-run -s \"-screen 0 1280x720x24\" playwright test --headed",
        "test:unit": "vitest run resources/js",
        "test:unit:single": "VITEST_JOBS=1 vitest run resources/js",
        "test:ci": "npm run test:unit:single && composer test",
        "prepare": "husky install",
        "test:unit:watch": "vitest --watch resources/js",
        "format": "prettier --write resources/",
        "format:check": "prettier --check resources/",
        "lint": "eslint . --fix",
        "release": "cd .. && ./scripts/release.sh",
        "release:patch": "cd .. && ./scripts/release.sh patch",
        "release:minor": "cd .. && ./scripts/release.sh minor",
        "release:major": "cd .. && ./scripts/release.sh major"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^20.2.0",
        "@eslint/js": "^9.19.0",
        "@laravel/vite-plugin-wayfinder": "^0.1.3",
        "@playwright/test": "^1.58.0",
        "@tailwindcss/vite": "^4.1.11",
        "@testing-library/vue": "^8.1.0",
        "@types/d3": "^7.4.3",
        "@types/node": "^22.13.5",
        "@vitejs/plugin-vue": "^6.0.0",
        "@vue/eslint-config-typescript": "^14.3.0",
        "@vue/test-utils": "^2.4.6",
        "commitlint": "^20.2.0",
        "concurrently": "^9.0.1",
        "cypress": "^15.9.0",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-vue": "^9.32.0",
        "husky": "^9.1.7",
        "jsdom": "^27.4.0",
        "msw": "^2.12.7",
        "prettier": "^3.4.2",
        "prettier-plugin-organize-imports": "^4.1.0",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "standard-version": "^9.5.0",
        "typescript": "^5.2.2",
        "typescript-eslint": "^8.23.0",
        "vite": "^7.0.4",
        "vitest": "^4.0.18",
        "vue-tsc": "^2.2.4"
    },
    "dependencies": {
        "@inertiajs/vue3": "^2.1.0",
        "@kyvg/vue3-notification": "^3.4.2",
        "@mdi/font": "^7.4.47",
        "@mdi/js": "^7.4.47",
        "@vueuse/core": "^12.8.2",
        "apexcharts": "^5.3.6",
        "chart.js": "^4.5.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "d3": "^7.9.0",
        "laravel-vite-plugin": "^2.0.0",
        "lucide-vue-next": "^0.468.0",
        "moment": "^2.30.1",
        "moments": "^0.0.2",
        "pinia": "^3.0.4",
        "reka-ui": "^2.4.1",
        "sass": "^1.94.2",
        "tailwind-merge": "^3.2.0",
        "tailwindcss": "^4.1.1",
        "tw-animate-css": "^1.2.5",
        "vue": "^3.5.13",
        "vue-chartjs": "^5.3.3",
        "vue3-apexcharts": "^1.10.0",
        "vuetify": "^3.11.2",
        "vuex": "^4.1.0"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.9.5",
        "@rollup/rollup-win32-x64-msvc": "4.9.5",
        "@tailwindcss/oxide-linux-x64-gnu": "^4.0.1",
        "@tailwindcss/oxide-win32-x64-msvc": "^4.0.1",
        "lightningcss-linux-x64-gnu": "^1.29.1",
        "lightningcss-win32-x64-msvc": "^1.29.1"
    }
}
